{
  "name": "ahauros-backend",
  "version": "1.0.0",
  "description": "Backend infrastructure pentru sistemul AI Ahauros",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docker:build": "docker build -t ahauros-backend .",
    "docker:run": "docker run -p 3000:3000 ahauros-backend",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "health:check": "node scripts/health-check.js"
  },
  "keywords": [
    "ai",
    "backend",
    "microservices",
    "postgresql",
    "redis",
    "clickhouse",
    "docker"
  ],
  "author": "Ahauros Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "clickhouse": "^2.6.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahauros/ahauros-backend.git"
  },
  "bugs": {
    "url": "https://github.com/ahauros/ahauros-backend/issues"
  },
  "homepage": "https://github.com/ahauros/ahauros-backend#readme"
}

